	pinctrl_pdm0: pinctrl_pdm0 {
		group0 {
			pinmux = < PIN_P6_7__PDM_C2_A >,
				 < PIN_P0_5__PDM_C0_A >,
				 < PIN_P0_7__PDM_C1_A >,
				 < PIN_P5_2__PDM_C3_A >;

			read_enable = < 0x0 >;
		};
		group1 {
			pinmux = < PIN_P5_4__PDM_D2_B >,
				 < PIN_P0_4__PDM_D0_A >,
				 < PIN_P0_6__PDM_D1_A >,
				 < PIN_P5_1__PDM_D3_A >;

			read_enable = < 0x1 >;
		};
	};

